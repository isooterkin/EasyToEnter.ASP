@model EasyToEnter.ASP.ViewsModels.Applicant.VariabilitySelectionContainerViewModel;

@*Библиотеки*@
@using EasyToEnter.ASP.ViewsModels.Applicant;
@using EasyToEnter.ASP.Components;
@using Newtonsoft.Json;
@using EasyToEnter.ASP.Models.Models;

@for(var i = 0; i < Model.VariabilityViewModelList.Count; i++)
{
    string ModelJson = Model.VariabilityViewModelList[i].Json();

    VariabilityViewModel? ModelNotJson = System.Text.Json.JsonSerializer.Deserialize<VariabilityViewModel>(ModelJson);

    <component type="typeof(VariabilitySelectionCard)" render-mode="ServerPrerendered" param-Model="ModelNotJson"/>
}